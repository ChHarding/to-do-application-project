        "def read_tasks(show_completed=False):\n",
        "    file_to_read = COMPLETED_TASKS_FILE if show_completed else CSV_FILE\n",
        "    tasks = []\n",
        "    with open(file_to_read, mode='r', newline='') as file:\n",
        "        reader = csv.DictReader(file)\n",
        "        for row in reader:\n",
        "            tasks.append(row)\n",
        "    return tasks"
